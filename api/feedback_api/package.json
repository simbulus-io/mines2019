{
  "name": "feedback_api",
  "version": "1.0.1",
  "main": "dist/bootstrap",
  "types": "dist/bootstrap",
  "scripts": {
    "start": "node ./dist/bootstrap",
    "dev": "npm run build:live",
    "test": "jest",
    "build": "./build.sh",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/bootstrap.ts"
  },
  "smokes": {
    "graphql_url": "http://localhost:5101/ts_node/v1.0/graphql"
  },
  "jest": {
    "reporters": [
      "default"
    ],
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec|smoke|smolder))\\.(ts|tsx|js)$",
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "modulePaths": [
      "node_modules",
      "../../../app/wm_express/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": false
  },
  "keywords": [],
  "author": "Sean Kelly",
  "dependencies": {
    "apollo-server": "^2.2.6",
    "apollo-server-express": "^2.2.6",
    "body-parser": "^1.18.3",
    "colors": "^1.3.3",
    "cookie-parser": "1.4.3",
    "dataloader": "^1.4.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "express-winston": "^3.0.1",
    "graphql": "^14.0.2",
    "graphql-middleware": "^2.0.2",
    "graphql-tools": "^4.0.3",
    "guid-typescript": "1.0.7",
    "http-server": "^0.11.1",
    "idx": "^2.5.2",
    "lodash": "^4.17.19",
    "logform": "^2.1.2",
    "md5": "^2.2.1",
    "mongodb": "3.0.7",
    "mysql": "^2.16.0",
    "node": "^8.11.2",
    "node-debug": "0.1.0",
    "object-hash": "^1.3.1",
    "prismjs": "^1.6.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "require-text": "0.0.1",
    "sha1": "^1.1.1",
    "sqlstring": "^2.3.1",
    "stack-trace": "^0.0.10",
    "stackblur-canvas": "^1.4.0",
    "ts-node": "^7.0.1",
    "winston": "^3.1.0",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.8",
    "@types/colors": "^1.2.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-graphql": "^0.6.1",
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.3.10",
    "@types/lodash": "^4.14.109",
    "@types/logform": "^1.2.0",
    "@types/md5": "^2.1.32",
    "@types/method-override": "0.0.31",
    "@types/mongodb": "3.0.17",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.1.4",
    "@types/node-fetch": "2.1.2",
    "@types/request": "^2.47.0",
    "@types/sqlstring": "^2.2.0",
    "@types/stack-trace": "^0.0.29",
    "@types/ws": "^6.0.1",
    "@types/yamljs": "0.2.30",
    "casual": "^1.5.19",
    "jest": "^22.4.4",
    "jest-html-reporter": "^2.4.2",
    "node-fetch": "^2.2.0",
    "random-name": "^0.1.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "typescript-tslint-plugin": "^0.2.1",
    "yarn": "^1.13.0"
  }
}
